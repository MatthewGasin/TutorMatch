<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tutormatchtest" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.NoActionBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main_student"
            android:label="@string/title_activity_main_student" >
        </activity>
        <activity
            android:name=".activity_enrolled"
            android:label="@string/title_activity_activity_enrolled" >
        </activity>
        <activity
            android:name=".activity_main_tutor"
            android:label="@string/title_activity_activity_main_tutor" >
        </activity>
        <activity
            android:name=".activity_student_find"
            android:label="@string/title_activity_activity_student_find" >
        </activity>
        <activity
            android:name=".activity_class_details"
            android:label="@string/title_activity_activity_class_details" >
        </activity>
    </application>

</manifest>
